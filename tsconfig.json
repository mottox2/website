{
  "include": [
    "./src/**/*",
    "./gatsby-node.ts"
  ],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["dom", "es2017"],
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": true,
    "jsx": "react",
    "jsxImportSource": "@emotion/react",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmit": true,
    "skipLibCheck": true
  }
}